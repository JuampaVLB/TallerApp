                <form id="enviar" method="POST" action="/barcodePost">

                    <section class="titulo_form">
                        <a href="/inicio">E.E.S.N°5</a>
                        <img width="30" src="/img/logo2.png">
                    </section>
                   
                    <input type="text" name="alumno" placeholder=" Alumno" required>
                    <input type="text" name="docente" placeholder=" Docente" required>
                  <div class="seleccion">
                    <select name = "especialidad_op" required>
                        <option>- -</option>
                        <option>Informatica</option>
                        <option>Turismo</option>
                        <option>Alimentos</option>
                        <option>Ciclo Basico</option>
                    </select>
                  </div>  
                  <div class="seleccion">
                    <select name = "year_op" required>
                        <option>- -</option>
                        <option>1 ro</option>
                        <option>2 do</option>
                        <option>3 ro</option>
                        <option>4 to</option>
                        <option>5 to</option>
                        <option>6 to</option>
                        <option>7 mo</option>
                    </select>
                  </div>
                  <div class="seleccion">
                    <select name = "year_div_op">
                        <option>- -</option>
                        <option>Primera</option>
                        <option>Segunda</option>
                        <option>Tercera</option>
                        <option>Cuarta</option>
                        <option>Quinta</option>
                        <option>Sexta</option>
                        <option>Septima</option>
                    </select>
                  </div>

                  <section class="form_botton">
                    <input type="submit" name="submit_form" value="Enviar">
                    <input type="reset" name="reset_form" value="Borrar">
                  </section>
            
                </form>

            </section>
        

    </div>
    <script src="//cdn.jsdelivr.net/npm/sweetalert2@11"></script>

    <% if (status === 'error') { %>
    
        <script>
            function Error() {
                Swal.fire({
                 title: 'ERROR',
                 text: 'CREDENCIONALES INCORRECTAS',
                 icon: 'error'
             });
            }

            Error();
        </script>

    <% } %>
